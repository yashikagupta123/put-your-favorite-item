#include<stdio.h>
main()
{
    printf("pick your favorite item:\n1.Pizza,Rs239\n2.Burger,Rs129\n3.Pasta,Rs179\n4.French Fries,Rs99\n5.Sandwich,Rs149\n");
	int choice=0;
	scanf("%d,&choice=0");
	switch(choice)
	{
		case 1:
			printf("Food-Pizza\nPrice-Rs239");
			break;
		case 2:
    		printf("Food-Burger\nPrice-Rs129");
    		break;
    	case 3:
    		printf("Food-Pasta\nPrice-Rs179");
    		break;
    	case 4:
    		printf("Food-French Fries\nPrice-Rs99");
    		break;
    	case 5:
    		printf("Food-Sandwich\nPrice-Rs149");
    		break;
    	default: printf("Invalid choice");
    		
	}
}
